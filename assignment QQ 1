#include<iostream>
using namespace std;
int main()
{
    // Assignment no#02.     Question no. 1st for input 7.
    // written by Ali Turi.
    int n = 7;
    for (int i = 1; i <= n; i++) {
        int count = 0;
        for (int j = i; j < n; j++) 
        {
            cout << j%n << " ";
            count++;
        }
        cout << n << " ";
        count++;
        for (int k = count; k < n; k++) 
        {
            cout << (i+k)%n << " ";
        }
        
        cout << endl;
    }
    return 0;
}
